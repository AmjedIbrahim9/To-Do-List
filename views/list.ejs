<%- include('header'); %> 

    <div class="box" id="heading">
        <h1><%= listTitle %> </h1>
        <p><%= time %> </p>
    </div>

    <div class="box">
        <% for (var i=0; i<ToDo.length; i++){ %> 
        <div class="item">
            <input type="checkbox">
            <p><%= ToDo[i] %></p>
        </div>
        <% } %>
        <form action="/" method="post" class="item">
            <input type="text" name="todoItem" placeholder="Add a ToDo Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %> >+</button>
        </form>
    </div>

    <%- include('footer'); %> 